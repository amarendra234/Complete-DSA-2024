class Solution {
  public:
    int maximumProfit(vector<int> &prices) {
        // code here
        
        int max_profit=0;
        for(int i=1;i<prices.size();i++){
             if(prices[i]>prices[i-1]){
                 max_profit+=(prices[i]-prices[i-1]);
             }
        }
       // max_profit+=profit;
        return max_profit;
    }
};
